{
  "todo": {
    "command": "todo",
    "subcommands": [
      "list",
      "add",
      "start",
      "end",
      "pause",
      "rename",
      "remove",
      "remove-all",
      "refresh",
      "next",
      "done"
    ]
  },
  "list": {
    "desc": "Output task list.",
    "alias": "ls",
    "options": [
      [
        "-l, --long",
        "Show long list."
      ]
    ]
  },
  "add": {
    "desc": "Add task.",
    "alias": "a",
    "arguments": [
      [
        "<names...>",
        "target task ids."
      ]
    ],
    "options": [
      [
        "-s, --start",
        "With started."
      ],
      [
        "-e, --end",
        "With ended."
      ]
    ]
  },
  "start": {
    "desc": "Start task.",
    "alias": "s",
    "arguments": [
      [
        "<ids...>",
        "target task ids."
      ]
    ]
  },
  "end": {
    "desc": "End task.",
    "alias": "e",
    "arguments": [
      [
        "<ids...>",
        "target task ids."
      ]
    ]
  },
  "pause": {
    "desc": "Pause task.",
    "alias": "p",
    "arguments": [
      [
        "[ids...]",
        "target task ids."
      ]
    ],
    "options": [
      [
        "-a, --all",
        "Pause all tasks."
      ]
    ]
  },
  "rename": {
    "desc": "Rename task.",
    "alias": "mv",
    "arguments": [
      [
        "<id>",
        "target task id."
      ],
      [
        "<name>",
        "new task name."
      ]
    ]
  },
  "remove": {
    "desc": "Remove task from list.",
    "alias": "rm",
    "arguments": [
      [
        "[ids...]",
        "target task ids."
      ]
    ],
    "options": [
      [
        "-e, --end",
        "Delete all ended tasks."
      ],
      [
        "-r, --refresh",
        "Delete all ended tasks."
      ]
    ]
  },
  "remove-all": {
    "desc": "Remove todo-file at home directory."
  },
  "refresh": {
    "desc": "Refresh all tasks id."
  },
  "next": {
    "desc": "Show current task."
  },
  "done": {
    "desc": "End current task."
  }
}